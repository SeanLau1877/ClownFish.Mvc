<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>
        @string.Concat(AppHelper.AppName, "-", this.Page.Title, " - ",
                           "http://www.cnblogs.com/fish-li", " - ", DEMO.Controllers.Page.BaseDemoController.CurrentStyle)
    </title>
    <link type="text/css" rel="Stylesheet" href="/static/js/jquery/jquery-easyui-1.2/themes/default/easyui.css" />
    <link type="text/css" rel="Stylesheet" href="/static/js/jquery/jquery-easyui-1.2/themes/icon.css" />
    @Html.Raw(UiHelper.RefCssFileHtml("/static/css/StyleSheet.css"))

    @RenderSection("head", required: false)
</head>
<body>

    <div id="topBanner">
        @Html.Raw(UcExecutor.Render("/Pages/Controls/PageHeader.ascx", this.Page.Title))
    </div>

    <div id="contentBody">
        @RenderBody()
    </div>

    @Html.Raw(UcExecutor.Render("/Pages/Controls/MainMenu.ascx", null))


    <p style="height: 53px" id="divBottomBlank"></p>

    <script type="text/javascript" src="/static/js/jquery/jquery-1.4.4.min.js"></script>
    <script type="text/javascript" src="/static/js/jquery/jquery-easyui-1.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="/static/js/jquery/jquery-easyui-1.2/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="/static/js/jquery/jquery.form.js"></script>

    @Html.Raw(UiHelper.RefJsFileHtml("/static/js/MyPage/fish.js"))
    @Html.Raw(UiHelper.RefJsFileHtml("/static/js/MyPage/dialog.js"))
    @Html.Raw(UiHelper.RefJsFileHtml("/static/js/MyPage/SetStyle.js"))

    @RenderSection("scripts", required: false)
</body>
</html>